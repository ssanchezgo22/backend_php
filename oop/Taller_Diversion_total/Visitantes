<?php

class Visitantes {
    private $nombre;
    private $edad;
    private $altura;
    private $telefono;
    private $email;
    private $condicion_espacial;
    private $tipo
}

public function __construct(
    $nombre, $edad, $altura, $telefono, $email = " ",
) {
    this -> nombre = $nombre;
    this -> edad = $edad;
    this -> altura = $altura;
    this -> telefono = $telefono;
    this -> email = $email;

}